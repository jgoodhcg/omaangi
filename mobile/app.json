{
  "expo": {
    "name": "umeng",
    "slug": "umeng",
    "privacy": "public",
    "entryPoint": "./app/index.js",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.0.27",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "dark",
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "dark",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "usesIcloudStorage": true,
      "bundleIdentifier": "com.jgoodhcg.umeng"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "appleTeamId": "85AM629RKZ",
          "iCloudContainerEnvironment": "Production"
        }
      ]
],
    "android": {
      "userInterfaceStyle": "dark",
      "package": "com.jgoodhcg.umeng",
      "versionCode": 3
    }
  }
}
